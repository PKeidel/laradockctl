{
    "name": "pkeidel/laradockctl",
    "description": "Allows to manage docker container for the development over cli - with help of the amazing laradock project",
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "PKeidel",
        "email": "PKeidel@sags-per-mail.de"
      }
    ],
    "require": {
        "php": "^8.1",
        "ext-yaml": "*",
        "ext-curl": "*",
        "jfcherng/php-diff": "*",
        "laravel/framework": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "PKeidel\\Laradockctl\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PKeidel\\Laradockctl\\Providers\\LaradockctlServiceProvider"
            ]
        }
    }
}
